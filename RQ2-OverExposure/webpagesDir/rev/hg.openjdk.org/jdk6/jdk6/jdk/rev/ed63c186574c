<span id="l12.36" class="plusline">+</span><a href="#l12.36"></a>
<span id="l12.37" class="plusline">+public final class TestEnumSet {</span><a href="#l12.37"></a>
<span id="l12.38" class="plusline">+    private static final Object OBJECT = new Object();</span><a href="#l12.38"></a>
<span id="l12.39" class="plusline">+</span><a href="#l12.39"></a>
<span id="l12.40" class="plusline">+    public static void main(String[] args) {</span><a href="#l12.40"></a>
<span id="l12.41" class="plusline">+        TestEncoder.test(</span><a href="#l12.41"></a>
<span id="l12.42" class="plusline">+                EnumSet.noneOf(Point.class),</span><a href="#l12.42"></a>
<span id="l12.43" class="plusline">+                new HashSet() {</span><a href="#l12.43"></a>
<span id="l12.44" class="plusline">+                    private final Object elementType = OBJECT;</span><a href="#l12.44"></a>
<span id="l12.45" class="plusline">+                },</span><a href="#l12.45"></a>
<span id="l12.46" class="plusline">+                OBJECT);</span><a href="#l12.46"></a>
<span id="l12.47" class="plusline">+    }</span><a href="#l12.47"></a>
<span id="l12.48" class="plusline">+</span><a href="#l12.48"></a>
<span id="l12.49" class="plusline">+    public enum Point { X, Y, Z }</span><a href="#l12.49"></a>
